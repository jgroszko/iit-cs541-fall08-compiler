#lang scheme

(require "helper.ss")

(add-tests-with-string-output "+"
			      [($+ 1 1) => "2"]
			      [($+ 1 2) => "3"]
			      [($+ 2 3) => "5"]
			      [($+ 536870911 -1) => "536870910"]
			      [($+ 536870910 1) => "536870911"]
			      [($+ -536870912 1) => "-536870911"]
			      [($+ -536870911 -1) => "-536870912"]
			      [($+ 536870911 -536870912) => "-1"]
			      [($+ ($+ ($+ 4 5) 10) 20) => "39"])

(add-tests-with-string-output "-"
			      [($- 1 1) => "0"]
			      [($- 1 2) => "-1"]
			      [($- 2 3) => "-1"]
			      [($- 536870911 1) => "536870910"]
			      [($- 536870910 -1) => "536870911"]
			      [($- -536870911 1) => "-536870912"]
			      [($- -536870912 -1) => "-536870911"]
			      [($- 536870911 536870912) => "-1"]
			      [($- ($- ($- 4 5) 10) 20) => "-31"])

(add-tests-with-string-output "*"
			      [($* 1 1) => "1"]
			      [($* 1 2) => "2"]
			      [($* 2 3) => "6"]
			      [($* 536870911 1) => "536870911"]
			      [($* 536870911 -1) => "-536870911"]
			      [($* -536870912 1) => "-536870912"]
			      [($* ($* ($* 4 5) 10) 20) => "4000"])

(add-tests-with-string-output "="
			      [($= 1 1) => "#t"]
			      [($= 1 2) => "#f"]
			      [($= 2 3) => "#f"]
			      [($= 536870911 1) => "#f"]
			      [($= -536870912 1) => "#f"]
			      [($= #t #t) => "#t"]
			      [($= #f #f) => "#t"]
			      [($= #t #f) => "#f"]
			      [($= #\A #\A) => "#t"]
			      [($= #\B #\z) => "#f"])

(add-tests-with-string-output "!="
			      [($!= 1 1) => "#f"]
			      [($!= 1 2) => "#t"]
			      [($!= 2 3) => "#t"]
			      [($!= 536870911 1) => "#t"]
			      [($!= -536870912 1) => "#t"]
			      [($!= #t #t) => "#f"]
			      [($!= #f #f) => "#f"]
			      [($!= #t #f) => "#t"]
			      [($!= #\A #\A) => "#f"]
			      [($!= #\B #\z) => "#t"])

(add-tests-with-string-output "Comparators"
			      [($>= 1 1) => "#t"]
			      [($> 1 1) => "#f"]
			      [($<= 1 1) => "#t"]
			      [($< 1 1) => "#f"]
			      [($>= 20 10) => "#t"]
			      [($> 20 10) => "#t"])
			      
